#include <stdio.h>

int main() {
    int n1, n2, i;
    
    printf("Enter the first number: ");
    scanf("%d", &n1);
    
    printf("Enter the second number: ");
    scanf("%d", &n2);
    
    printf("Odd numbers between %d and %d are:\n", n1, n2);
    for (i = n1; i <= n2; i++) {
        if (i % 2 != 0) {
            printf("%d\n", i);
        }
    }
    
    return 0;
}
